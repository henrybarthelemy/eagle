name x
name k_data 
name psk

def alice_main () =     
    let c = enc(get(psk), get(k_data)) in
    output ret(c) in
    input i in
    let res = dec(get(k_data), i) in 
    match res
    | Something a => ret(res),
    | Nothing => ret(unit) 
end def
    
def bob_main () =
    input i in 
    let res = dec(get(psk), i) in
    match res
     | Some k => let c = enc(k, get(x)) in 
     output ret(c),
     | None => ret(unit)
end def